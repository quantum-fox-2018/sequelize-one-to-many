<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/css.table.ejs')  %>
  </head>
  <body>

    <h2>Data Students</h2>
    <h4><a href="/">Home</a></h4>
    <h4><a href="/students/add">Add Student</a></h4>

    <table>
      <tr>
        <th>id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Options</th>
      </tr>
        <%
          data_students.forEach(function(student){%>
          <tr>
            <td><%= student.id%></td>
            <td><%= student.first_name%></td>
            <td><%= student.last_name%></td>
            <td><a href="/students/edit/<%= student.id%>">edit</a> | <a href="/students/delete/<%= student.id%>" onclick="return confirm('Are you sure want delete this student?')">delete</a></td>
          </tr>
          <%})
        %>
    </table>

  </body>
</html>
