<div class="table-responsive">
  <table class="table">
    <tr>
      <th>id</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Options</th>
    </tr>
    <% students.forEach( student => {%>
    <tr>
      <td><%= student.id %></td>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.email %></td>
      <td>
        <a href="/students/edit/<%= student.id %>">Edit </a>|
        <a href="/students/delete/<%= student.id %>" onclick="return confirm('Are you sure want to delete this data?');"> Delete</a>
      </td>
    </tr>
    <% })%>
  </table>
</div>
